<!DOCTYPE html>
<html>
<head>
    <title>AJAX Random Quote (backup)</title>
</head>
<body>

<h2>Random Quote Generator</h2>

<button onclick="loadQuote()">Load Quote</button>

<p id="quote"></p>

<script>
function loadQuote() {
    fetch("quote.txt")
        .then(response => response.text())
        .then(data => {
            document.getElementById("quote").innerText = data;
        });
}
</script>

</body>
</html>
